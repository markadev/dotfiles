### This file configures various client-side behaviors.

[miscellany]
enable-auto-props = yes

# Some automatic properties when a file is added based on the file type
[auto-props]
*.txt  = svn:mime-type=text/plain
*.xml  = svn:mime-type=text/xml
*.asc  = svn:mime-type=text/plain
*.htm  = svn:mime-type=text/html
*.html = svn:mime-type=text/html
*.shtml = svn:mime-type=text/html
*.css  = svn:mime-type=text/css
*.jpg  = svn:mime-type=image/jpeg
*.jpeg = svn:mime-type=image/jpeg
*.tif  = svn:mime-type=image/tiff
*.tiff = svn:mime-type=image/tiff
*.png  = svn:mime-type=image/png
*.gif  = svn:mime-type=image/gif
*.ico  = svn:mime-type=image/x-icon
*.bmp  = svn:mime-type=image/bmp
*.swf  = svn:mime-type=application/x-shockwave-flash
*.blend = svn:mime-type=application/blender
*.xcf  = svn:mime-type=application/x-gimp-image
*.xm   = svn:mime-type=audio/x-xm
*.xi   = svn:mime-type=audio/x-xi
*.mod  = svn:mime-type=audio/x-mod
*.wav  = svn:mime-type=audio/x-wav
*.mid  = svn:mime-type=audio/midi
*.zip  = svn:mime-type=application/zip
*.pdf  = svn:mime-type=application/pdf
*.ogg  = svn:mime-type=application/ogg

*.odc  = svn:mime-type=application/vnd.oasis.opendocument.chart
*.odf  = svn:mime-type=application/vnd.oasis.opendocument.formula
*.odg  = svn:mime-type=application/vnd.oasis.opendocument.graphics
*.odi  = svn:mime-type=application/vnd.oasis.opendocument.image
*.odp  = svn:mime-type=application/vnd.oasis.opendocument.presentation
*.ods  = svn:mime-type=application/vnd.oasis.opendocument.spreadsheet
*.odt  = svn:mime-type=application/vnd.oasis.opendocument.text
*.ott  = svn:mime-type=application/vnd.oasis.opendocument.text-template

*.c    = svn:mime-type=text/x-csrc
*.h    = svn:mime-type=text/x-chdr
*.cpp  = svn:mime-type=text/x-c++src
*.cxx  = svn:mime-type=text/x-c++src;
*.py   = svn:mime-type=text/x-python
*.java = svn:mime-type=text/x-java
*.jar  = svn:mime-type=application/java-archive
*.ser  = svn:mime-type=application/java-serialized-object
*.class = svn:mime-type=application/java-vm

waf = svn:mime-type=application/octet-stream
